package travelers;

import java.lang.Number;

rule "PremiumValidations"
	dialect "mvel"
	ruleflow-group "premiumvalidation"
	when
		Bond( )
		Transaction( commissionrate > 10 , specialcommission > 500 )
		nbe : NewBondEligible( )
	then
		modify( nbe ) {
				setEligible( true ), 
				setReason( "Passed Premium Validation" )
		}
end
